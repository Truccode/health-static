              <option value="male">Nam</option>
              <option value="female">Nữ</option>
            </select>
            <input id="weight" type="number" step="0.1" value="70" class="border p-2 rounded" placeholder="Cân nặng (kg)" />
            <input id="height" type="number" step="0.1" value="170" class="border p-2 rounded" placeholder="Chiều cao (cm)" />
            <select id="activity" class="border p-2 rounded col-span-2">
              <option value="1.2">Ít vận động</option>
              <option value="1.375">Vận động nhẹ</option>
