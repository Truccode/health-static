<script>
  document.addEventListener("DOMContentLoaded", () => {
  const sidebar = document.querySelector("nav.mt-6");
  const buttons = sidebar.querySelectorAll("button[data-target]");
  
  // Tạo thanh trắng di chuyển
  const activeBg = document.createElement("div");
  activeBg.classList.add("active-bg");
  sidebar.style.position = "relative";
  sidebar.appendChild(activeBg);

  // Cập nhật vị trí thanh trắng
  function moveHighlight(btn) {
    const rect = btn.getBoundingClientRect();
    const parentRect = sidebar.getBoundingClientRect();
    activeBg.style.top = (btn.offsetTop) + "px";

    buttons.forEach(b => b.classList.remove("bg-white", "text-blue-700", "active"));
    btn.classList.add("bg-white", "text-blue-700", "active");
  }

  // Ẩn hiện nội dung theo data-target
  function showSection(targetId) {
    document.querySelectorAll("main > section").forEach(sec => {
      sec.style.display = sec.id === targetId ? "block" : "none";
    });
  }

  // Gán sự kiện click
  buttons.forEach(btn => {
    btn.addEventListener("click", () => {
      moveHighlight(btn);
      showSection(btn.dataset.target);
    });
  });

  // Đặt vị trí ban đầu
  const firstActive = sidebar.querySelector("button.active");
  if (firstActive) {
    activeBg.style.top = firstActive.offsetTop + "px";
    showSection(firstActive.dataset.target);
  }
});
// --- Sample foods database (per 100g) ---
const FOODS = [
  {id:'rice', name:'Cơm trắng', kcal:130, protein:2.7, carbs:28, fat:0.3},
  {id:'chicken', name:'Ức gà', kcal:165, protein:31, carbs:0, fat:3.6},
  {id:'egg', name:'Trứng (100g)', kcal:155, protein:13, carbs:1.1, fat:11},
  {id:'fish', name:'Cá', kcal:206, protein:22, carbs:0, fat:12},
  {id:'milk', name:'Sữa tươi', kcal:60, protein:3.2, carbs:4.8, fat:1.5},
  {id:'banana', name:'Chuối', kcal:89, protein:1.1, carbs:23, fat:0.3},
  {id:'bread', name:'Bánh mì (100g)', kcal:265, protein:9, carbs:49, fat:3.2},
];

// Utility functions
function calcBMR({age,gender,weight,height}){
  const s = gender==='male'?5:-161;
  const bmr = 10*weight + 6.25*height - 5*age + s;
  return Math.round(bmr);
}
function calcTDEE(bmr, activity){ return Math.round(bmr * Number(activity)); }
function calcBMI(weight, height){ if(!weight||!height) return null; const h = height/100; const bmi = weight/(h*h); return Math.round(bmi*10)/10; }
function bmiCategory(bmi){ if(bmi===null) return ''; if(bmi<18.5) return 'Gầy'; if(bmi<25) return 'Bình thường'; if(bmi<30) return 'Thừa cân'; return 'Béo phì'; }

// State
let mealItems = JSON.parse(localStorage.getItem('meal_items') || '[]');
const foodSelect = document.getElementById('foodSelect');
const mealList = document.getElementById('mealList');
const totalsDiv = document.getElementById('totals');
const pieCtx = document.getElementById('pieChart').getContext('2d');
const barCtx = document.getElementById('barChart').getContext('2d');

// Populate food select
FOODS.forEach(f => {
  const op = document.createElement('option');
  op.value = f.id; op.textContent = f.name;
  foodSelect.appendChild(op);
});

// Panels navigation
document.querySelectorAll('[data-target]').forEach(btn=>{
  btn.addEventListener('click', ()=>{
    const target = btn.getAttribute('data-target');
    document.querySelectorAll('.panel').forEach(p=>p.classList.add('hidden'));
    document.getElementById(target).classList.remove('hidden');
    document.querySelectorAll('[data-target]').forEach(b=>b.classList.remove('active'));
    // mark matching sidebar button active
    document.querySelectorAll('[data-target]').forEach(b=>{ if(b.getAttribute('data-target')===target) b.classList.add('active'); });
    // refresh charts when showing reports
    if(target==='reports') updateBarChart();
  });
});

function calcWater() {
  const weight = parseFloat(document.getElementById("waterWeight").value);
  const activity = document.getElementById("waterActivity").value;
  const hot = document.getElementById("waterHot").checked;
  const resultDiv = document.getElementById("waterResult");

  if (!weight || weight <= 0) {
    resultDiv.innerHTML = `<p class='text-red-600 font-medium'>⚠️ Vui lòng nhập cân nặng hợp lệ!</p>`;
    return;
  }

  let total = weight * 35;
  if (activity === "vua") total += 500;
  else if (activity === "nhieu") total += 1000;
  if (hot) total += 500;

  const liters = (total / 1000).toFixed(2);
  const cups = (total / 250).toFixed(1);

  resultDiv.innerHTML = `
    <h3 class='text-3xl font-bold text-blue-700'>${total} ml</h3>
    <p class='text-slate-600'>~ ${liters} lít nước mỗi ngày</p>
    <p class='text-slate-500 text-sm'>Tương đương khoảng ${cups} cốc (250ml)</p>
  `;
}

// Calculator handlers
document.getElementById('calcBtn').addEventListener('click', ()=>{
  const age = Number(document.getElementById('age').value);
  const gender = document.getElementById('gender').value;
  const weight = Number(document.getElementById('weight').value);
  const height = Number(document.getElementById('height').value);
  const activity = document.getElementById('activity').value;
  const goal = document.getElementById('goal').value;

  const bmr = calcBMR({age,gender,weight,height});
  const tdee = calcTDEE(bmr, activity);
  let recommended = tdee;
  if(goal==='lose') recommended = Math.max(1200, tdee - 500);
  if(goal==='gain') recommended = tdee + 500;

  const bmi = calcBMI(weight, height);
  const cat = bmiCategory(bmi);

  const out = document.getElementById('calcResult');
  out.innerHTML = `
    <div class="p-4 bg-white rounded shadow">
      <div class="text-sm text-slate-500">BMR</div>
      <div class="font-semibold text-lg">${bmr} kcal/ngày</div>
    </div>
    <div class="p-4 bg-white rounded shadow">
      <div class="text-sm text-slate-500">TDEE (trước mục tiêu)</div>
      <div class="font-semibold text-lg">${tdee} kcal/ngày</div>
    </div>
    <div class="p-4 bg-white rounded shadow">
      <div class="text-sm text-slate-500">Mục tiêu</div>
      <div class="font-semibold text-lg">${goal==='lose'?'Giảm cân': goal==='gain'?'Tăng cân':'Giữ cân'}</div>
    </div>
    <div class="p-4 bg-white rounded shadow">
      <div class="text-sm text-slate-500">Khuyến nghị</div>
      <div class="font-semibold text-lg">${recommended} kcal/ngày</div>
    </div>
    <div class="p-4 bg-white rounded shadow col-span-2">
      <div class="text-sm text-slate-500">BMI</div>
      <div class="font-semibold text-lg">${bmi} — ${cat}</div>
    </div>
  `;
  // store recommended to localStorage for reports
  localStorage.setItem('last_recommend', JSON.stringify({recommended, tdee}));
  updateBarChart();
});

document.getElementById('clearCalc').addEventListener('click', ()=>{
  document.getElementById('age').value=''; document.getElementById('gender').value='male';
  document.getElementById('weight').value=''; document.getElementById('height').value=''; document.getElementById('calcResult').innerHTML='';
});

// Meal functions
function renderMeal(){
  mealList.innerHTML='';
  if(mealItems.length===0){
    mealList.innerHTML = '<div class="small">Chưa có món nào</div>';
  } else {
    mealItems.forEach(it=>{
      const div = document.createElement('div');
      div.className='p-2 border-b flex justify-between items-center';
      div.innerHTML = `<div><div class="font-semibold">${it.name} • ${it.grams} g</div><div class="small">${it.kcal} kcal • P ${it.protein}g • C ${it.carbs}g • F ${it.fat}g</div></div>`;
      const btn = document.createElement('button'); btn.className='border px-2 py-1 rounded'; btn.textContent='Xóa';
      btn.addEventListener('click', ()=>{ mealItems = mealItems.filter(x=>x.id!==it.id); saveMeal(); renderMeal(); updateCharts(); });
      div.appendChild(btn);
      mealList.appendChild(div);
    });
  }
  renderTotals();
}

// add food
document.getElementById('addFood').addEventListener('click', ()=>{
  const id = document.getElementById('foodSelect').value;
  const grams = Number(document.getElementById('foodGram').value) || 0;
  const food = FOODS.find(f=>f.id===id);
  if(!food) return;
  const factor = grams/100;
  const entry = { id: Date.now()+Math.random(), name:food.name, grams, kcal: Math.round(food.kcal*factor), protein: +(food.protein*factor).toFixed(1), carbs: +(food.carbs*factor).toFixed(1), fat: +(food.fat*factor).toFixed(1) };
  mealItems.unshift(entry);
  saveMeal(); renderMeal(); updateCharts(); updateBarChart();
});

function saveMeal(){ localStorage.setItem('meal_items', JSON.stringify(mealItems)); }

document.getElementById('clearMeal').addEventListener('click', ()=>{ if(confirm('Xóa toàn bộ món ăn?')){ mealItems=[]; saveMeal(); renderMeal(); updateCharts(); updateBarChart(); }});

document.getElementById('exportMeal').addEventListener('click', ()=>{
  const blob = new Blob([JSON.stringify(mealItems,null,2)], {type:'application/json'});
  const url = URL.createObjectURL(blob); const a = document.createElement('a'); a.href=url; a.download=`meal_${new Date().toISOString().slice(0,10)}.json`; a.click(); URL.revokeObjectURL(url);
});
document.getElementById('importBtn').addEventListener('click', ()=> document.getElementById('importFile').click());
document.getElementById('importFile').addEventListener('change', (e)=>{
  const f = e.target.files[0]; if(!f) return;
  const r = new FileReader(); r.onload = ()=>{ try{ const parsed = JSON.parse(r.result); if(Array.isArray(parsed)){ mealItems = parsed; saveMeal(); renderMeal(); updateCharts(); updateBarChart(); alert('Import thành công'); } else alert('Tệp không đúng định dạng'); }catch(err){ alert('Lỗi đọc tệp'); } }; r.readAsText(f);
});

function renderTotals(){
  const totals = mealItems.reduce((acc,it)=>{ acc.kcal+=it.kcal; acc.protein+=it.protein; acc.carbs+=it.carbs; acc.fat+=it.fat; return acc; }, {kcal:0,protein:0,carbs:0,fat:0});
  totalsDiv.innerHTML = `
    <div class="p-2 bg-slate-50 rounded">Calo<div class="font-semibold">${totals.kcal} kcal</div></div>
    <div class="p-2 bg-slate-50 rounded">Protein<div class="font-semibold">${Math.round(totals.protein*10)/10} g</div></div>
    <div class="p-2 bg-slate-50 rounded">Carbs<div class="font-semibold">${Math.round(totals.carbs*10)/10} g</div></div>
    <div class="p-2 bg-slate-50 rounded">Fat<div class="font-semibold">${Math.round(totals.fat*10)/10} g</div></div>
  `;
}

// Charts
let pieChart = null;
let barChart = null;
function updateCharts(){
  const totals = mealItems.reduce((acc,it)=>{ acc.kcal+=it.kcal; acc.protein+=it.protein; acc.carbs+=it.carbs; acc.fat+=it.fat; return acc; }, {kcal:0,protein:0,carbs:0,fat:0});
  const pieData = [totals.protein, totals.carbs, totals.fat];
  if(pieChart) { pieChart.data.datasets[0].data = pieData; pieChart.update(); }
  else {
    pieChart = new Chart(pieCtx, { type:'pie', data:{ labels:['Protein','Carbs','Fat'], datasets:[{ data: pieData, backgroundColor:['#2563eb','#60a5fa','#34d399'] }] } });
  }
}
function updateBarChart(){
  const last = JSON.parse(localStorage.getItem('last_recommend')||'null');
  const recommended = last?last.recommended:0;
  const totals = mealItems.reduce((acc,it)=>acc+it.kcal,0);
  if(barChart){ barChart.data.datasets[0].data = [totals, recommended]; barChart.update(); }
  else {
    barChart = new Chart(barCtx, { type:'bar', data:{ labels:['Tiêu thụ','Mục tiêu'], datasets:[{ label:'kcal', data:[totals,recommended], backgroundColor:['#2563eb','#60a5fa'] }] }, options:{ scales:{ y:{ beginAtZero:true } } } });
  }
}

// init
renderMeal(); updateCharts(); updateBarChart();

// show home by default
document.querySelectorAll('.panel').forEach(p=>p.classList.add('hidden'));
document.getElementById('home').classList.remove('hidden');
/*************************************************
 * REPORTS SCRIPT – BÁO CÁO DINH DƯỠNG
 *************************************************/

/**
 * Render báo cáo tổng hợp
 */
function renderReport() {
  // Lấy dữ liệu từ Calculator
  const last = JSON.parse(localStorage.getItem('last_recommend') || 'null');
  if (!last) {
    setEmptyReport();
    return;
  }

  const tdee = last.tdee;
  const target = last.recommended;

  // Tổng calo đã ăn
  const consumed = mealItems.reduce((sum, it) => sum + it.kcal, 0);
  const diff = target - consumed;

  /* ===== KPI ===== */
  setText("rpTDEE", tdee + " kcal");
  setText("rpTarget", target + " kcal");
  setText("rpConsumed", consumed + " kcal");

  const diffEl = document.getElementById("rpDiff");
  diffEl.innerText = (diff >= 0 ? "+" : "") + diff + " kcal";
  diffEl.className =
    "text-2xl font-bold " +
    (diff >= 0 ? "text-green-600" : "text-red-600");

  /* ===== PHÂN TÍCH ===== */
  let analysis = `
    Tổng lượng calo đã tiêu thụ trong ngày là
    <strong>${consumed} kcal</strong>,
    trong khi mức năng lượng mục tiêu được khuyến nghị là
    <strong>${target} kcal</strong>.
  `;

  if (diff > 0) {
    analysis += `
      Người dùng hiện đang <strong>ăn ít hơn mục tiêu</strong>,
      phù hợp với chiến lược giảm cân hoặc kiểm soát cân nặng.
    `;
  } else if (diff < 0) {
    analysis += `
      Lượng calo nạp vào đã <strong>vượt mức khuyến nghị</strong>,
      có nguy cơ ảnh hưởng đến mục tiêu sức khỏe.
    `;
  } else {
    analysis += `
      Lượng calo tiêu thụ hiện tại <strong>đúng bằng mục tiêu</strong>,
      đây là trạng thái cân bằng lý tưởng.
    `;
  }

  document.getElementById("rpAnalysis").innerHTML = analysis;

  /* ===== KẾT LUẬN & KHUYẾN NGHỊ ===== */
  const advice = document.getElementById("rpAdvice");
  advice.innerHTML = "";

  if (diff > 0) {
    advice.innerHTML += "<li>Có thể bổ sung thêm bữa nhẹ lành mạnh (trái cây, sữa chua)</li>";
    advice.innerHTML += "<li>Duy trì chế độ ăn và luyện tập hiện tại</li>";
  } 
  else if (diff < 0) {
    advice.innerHTML += "<li>Giảm lượng tinh bột và đồ ăn nhiều năng lượng</li>";
    advice.innerHTML += "<li>Tăng cường vận động thể chất (đi bộ, cardio)</li>";
  } 
  else {
    advice.innerHTML += "<li>Tiếp tục duy trì chế độ ăn hiện tại</li>";
    advice.innerHTML += "<li>Theo dõi và đánh giá lại sau mỗi ngày</li>";
  }
}

/**
 * Khi chưa có dữ liệu
 */
function setEmptyReport() {
  setText("rpTDEE", "--");
  setText("rpTarget", "--");
  setText("rpConsumed", "--");
  setText("rpDiff", "--");

  document.getElementById("rpAnalysis").innerText =
    "Chưa có đủ dữ liệu để tạo báo cáo. Vui lòng tính calo và thêm món ăn.";

  document.getElementById("rpAdvice").innerHTML =
    "<li>Nhập thông tin cá nhân và tính TDEE</li>" +
    "<li>Thêm món ăn vào danh sách trong ngày</li>";
}

/**
 * Helper set text
 */
function setText(id, value) {
  const el = document.getElementById(id);
  if (el) el.innerText = value;
}

/*************************************************
 * GẮN VÀO LUỒNG HOẠT ĐỘNG HIỆN CÓ
 *************************************************/

// Khi mở tab Báo cáo
document.querySelectorAll('[data-target]').forEach(btn => {
  btn.addEventListener('click', () => {
    if (btn.getAttribute('data-target') === 'reports') {
      renderReport();
    }
  });
});

</script>
</body>
</html>
